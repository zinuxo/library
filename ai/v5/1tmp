IF
Def And 2 2 Bi
	I-Bi ;
	I-Bi ;
	Re '0' If v0 = '0' ;
	Re v1 ;
End

IF
Def Xor 2 2 Bi
	I-Bi ;
	I-Bi ;
	Re '0' If v0 = v1 ;
	Re '1' ;
End

IF
Def Or 2 2 Bi
	I-Bi ;
	I-Bi ;
	Re v1 If v0 = '0' ;
	Re '1' ;
End

IF
Def Not 1 2 Bi
	I-Bi ;
	Re '1' If v0 = '0' ;
	Re '0' ;
End

IF
Def GetE 0 1 L
	Re 'E' ;
End

IF
Def ToL 1 1 L
	I-Bi ;
	GetE ;
	C v0 v1 ;
	Re v2 ;
End

IF
Def Mul01' 2 3 L
	I-Bi ;
	I-L ;
	Re v1 If v0 = '1' ;
	ToL v0 ;
	Re v3 ;
End

IF
Def Plus01 2 3 L
	I-Bi ;
	I-L ;
	Re v1 If v0 = '0' ;
	C v0 v1 ;
	Re v3 If v1 = 'E' ;
	L v1 ;
	R v1 ;
	Xor v0 v5 ;
	And v0 v5 ;
	Plus01 v8 v6 ;
	C v7 v9 ;
	Re v10 ;
End



